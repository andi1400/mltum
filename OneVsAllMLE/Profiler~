#!/usr/bin/env python
# encoding: utf-8
# filename: profile.py

import pstats, cProfile

import logisticGeneral

cProfile.runctx("logisticGeneral", globals(), locals(), "Profile.prof")

s = pstats.Stats("Profile.prof")
s.sort_stats("time").print_stats()
s.strip_dirs().sort_stats("time").print_stats()


cProfile.run('logisticGeneral.run()', 'profile.prof')
